#include <stdio.h>

int main() {
    int numbers[5];
    int max, i;

    printf("Enter 5 integers:\n");
    for(i = 0; i < 5; i++) {
        scanf("%d", &numbers[i]);
    
    max = numbers[0];
    for(i = 1; i < 5; i++) {
        if(numbers[i] > max) {
            max = numbers[i];
        }
    }
    printf("The maximum number is: %d\n", max);
    return 0;
}
